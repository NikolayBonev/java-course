import java.util.Scanner;

public class OneThousandNums {

	public static void main(String[] args) {
		Scanner inputs = new Scanner(System.in, "UTF-8");
		
		System.out.print("Enter a number: ");
		int inputNum = inputs.nextInt();
		
		int hundreds = inputNum/100;
		System.out.println(hundreds);
		inputNum = inputNum%100;
		int tenths, units;
		if(inputNum>=1 && inputNum<=9){
			tenths = 0;
			System.out.println(tenths);
			units = inputNum;
			System.out.println(units);
		}else{
			tenths = inputNum/10;
			System.out.println(tenths);
			units = inputNum%10;
			System.out.println(units);
		}
		
		boolean hunF=false, tenF=false, uniF=true, flagi=false, flagi2=false;
		if(hundreds>=1 && hundreds<=9){
			hunF=true;
		}else if(hundreds==0){
			hunF=false;
		}else if(hundreds>9){
			System.out.println("Out of range");
			hunF=tenF=uniF=false;
		}
		if(tenths>1 && tenths<=9){
			tenF=true;
		}else if(tenths==1){
			tenF=true;
			uniF=false;
		} else if(tenths==0){
			tenF=false;
		} 
		if(units==0){
			uniF=false;
		}
		
		while((hunF || tenF) || uniF){
			//System.out.println(hunF + "   " + tenF + "   " + uniF);
			
			if(flagi2==true){
				break;
			}
			if(hunF==true && hundreds>=1 && hundreds<=9 && flagi==true){
				System.out.print(" hundread");
				hunF=false;
				if(tenF==false && uniF==true){
					System.out.print(" and ");
					flagi2=true;
				}
			}

			if((hunF == false && tenF == false && uniF==false)){
				//System.out.println(hunF + "   " + tenF + "   " + uniF);
				break;
			}
			if(uniF || hunF){
				
				if(hunF==true){
					flagi=true;
				}
				
				switch (hundreds) {
				case 1:
					System.out.print("One");
					continue;
				case 2:
					System.out.print("Two");
					continue;
				case 3:
					System.out.print("Three");
					continue;
				case 4:
					System.out.print("Four");
					continue;
				case 5:
					System.out.print("Five");
					continue;
				case 6:
					System.out.print("Six");
					continue;
				case 7:
					System.out.print("Seven");
					continue;
				case 8:
					System.out.print("Eight");
					continue;
				case 9:
					System.out.print("Nine");
					continue;
				}
			}
			
			if(tenF){
				tenF=false;
				if(tenths==1){
					System.out.print(" and");
					switch (units) {
					case 0:
						System.out.print(" ten");
						break;
					case 1:
						System.out.print(" eleven");
						break;
					case 2:
						System.out.print(" twelve");
						break;
					case 3:
						System.out.print(" thirteen");
						break;
					case 4:
						System.out.print(" fourteen");
						break;
					case 5:
						System.out.print(" fifteen");
						break;
					case 6:
						System.out.print(" sixteen");
						break;
					case 7:
						System.out.print(" seventeen");
						break;
					case 8:
						System.out.print(" eighteen");
						break;
					case 9:
						System.out.print(" nineteen");
						break;
					}
				}
				switch (tenths) {
				case 2:
					System.out.print(" twenty");
					continue;
				case 3:
					System.out.print(" thirty");
					continue;
				case 4:
					System.out.print(" forty");
					continue;
				case 5:
					System.out.print(" fifty");
					continue;
				case 6:
					System.out.print(" sixty");
					continue;
				case 7:
					System.out.print(" seventy");
					continue;
				case 8:
					System.out.print(" eighty");
					continue;
				case 9:
					System.out.print(" ninety");
					continue;
					
				}
			}
			
		}
		
		inputs.close();

	}

}
