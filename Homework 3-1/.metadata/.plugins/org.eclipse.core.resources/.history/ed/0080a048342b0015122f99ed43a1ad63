import java.util.Scanner;

public class DeckOfCards {

	public static void main(String[] args) {
		Scanner inputs = new Scanner(System.in, "UTF-8");
		
		System.out.print("Enter a number from the deck: ");
		int cardNumDeck;
		
		if(inputs.hasNext("[AJKQajkq]")){
			System.out.println("Валидна карта!");
		}else if(inputs.hasNextInt()){
			cardNumDeck=inputs.nextInt();
			
			if((cardNumDeck >= 2 && cardNumDeck <= 10) || (cardNumDeck==(int) 'A' || cardNumDeck==(int) 'J' || cardNumDeck=='Q' || cardNumDeck=='K')){
				System.out.println("Валидна карта!");
			}else{
				System.out.println("Невалидна карта!");
			}
		}else{
			System.out.println("Невалидна карта!");
		}
		
		inputs.close();
	}

}
