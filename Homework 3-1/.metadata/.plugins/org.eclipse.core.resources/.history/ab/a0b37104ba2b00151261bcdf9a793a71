import java.util.Scanner;

public class OneThousandNums {

	public static void main(String[] args) {
		Scanner inputs = new Scanner(System.in, "UTF-8");
		
		System.out.print("Enter a number: ");
		int inputNum = inputs.nextInt();

		int hundreds = inputNum/100;
		System.out.println(hundreds);
		inputNum = inputNum%100;
		int tenths, units;
		if(inputNum>=1 && inputNum<=9){
			tenths = 0;
			System.out.println(tenths);
			units = inputNum;
			System.out.println(units);
		}else{
			tenths = inputNum/10;
			System.out.println(tenths);
			units = inputNum%10;
			System.out.println(units);
		}
		
		boolean hunF=false, tenF=false, uniF=false, ifhunF=false, ifuniF=false;
		if(hundreds>=1 && hundreds<=9){
			hunF=true;
			ifhunF=true;
		}else if(hundreds==0){
			hunF=false;
		}else if(hundreds>9){
			System.out.println("Invalid input");
			hunF=false;
			tenF=false;
			uniF=false;
		}
		if(tenths>=1 && tenths<=9){
			tenF=true;
		}else if(tenths==0){
			tenF=false;
			if(units==0 && hundreds==0){
				System.out.println("Zero!");
				uniF=false;
			}
		}
		if(units!=0){
			uniF=true;
		}else{
			uniF=false;
		}
		while(uniF || tenF || hunF){
			System.out.print(uniF + " " +tenF + " " + hunF);
			if(hunF==true && hundreds>=1 && hundreds<=9 && ifhunF==false){
				hunF=false;
				System.out.print(" hundred");
				
				if(tenths == 1 && uniF==false){
					System.out.print(" and ");
					ifuniF=false;
					
				}else if(tenF==false && uniF==true){
					ifuniF=true;
					System.out.print(" and ");
					
				}
			}
			
			if((uniF ==true && ifuniF==true) || (hunF==true && ifhunF==true)){
				if(hunF==true){
					ifhunF=false;
				}
				if(ifuniF==true){
					uniF=false;
				}
				switch (hundreds) {
				case 1:
					System.out.print("One");
					continue;
				case 2:
					System.out.print("Two");
					continue;
				case 3:
					System.out.print("Three");
					continue;
				case 4:
					System.out.print("Four");
					continue;
				case 5:
					System.out.print("Five");
					continue;
				case 6:
					System.out.print("Six");
					continue;
				case 7:
					System.out.print("Seven");
					continue;
				case 8:
					System.out.print("Eight");
					continue;
				case 9:
					System.out.print("Nine");
					continue;
				}
			}
			
			if(tenF){
				if(tenths==1){
					uniF=false;
					System.out.print(uniF + " " +tenF + " " + hunF);
					break;
					switch (units) {
					case 0:
						System.out.print(" ten");
						break;
					case 1:
						System.out.print(" eleven");
						break;
					case 2:
						System.out.print("Twelve");
						break;
					case 3:
						System.out.print("Three1");
						break;
					case 4:
						System.out.print("Four1");
						break;
					case 5:
						System.out.print("Five1");
						break;
					case 6:
						System.out.print("Six1");
						break;
					case 7:
						System.out.print("Seven1");
						break;
					case 8:
						System.out.print("Eight1");
						break;
					case 9:
						System.out.print("Nine1");
						break;
					}
				}
				if(uniF==true){
					ifuniF=true;
				}
				switch (tenths) {
				case 2:
					System.out.print("Two2");
					continue;
				case 3:
					System.out.print("Three2");
					continue;
				case 4:
					System.out.print("Four2");
					continue;
				case 5:
					System.out.print("Five2");
					continue;
				case 6:
					System.out.print("Six2");
					continue;
				case 7:
					System.out.print("Seven2");
					continue;
				case 8:
					System.out.print("Eight2");
					continue;
				case 9:
					System.out.print("Nine2");
					continue;
				}
			}
		}
		
		
		inputs.close();
	}

}
