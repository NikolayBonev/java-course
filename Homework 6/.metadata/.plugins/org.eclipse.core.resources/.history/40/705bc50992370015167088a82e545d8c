package market;

public class Drinks extends Stock{
	
	private double alcoholPer;
	private boolean carbonate;
	
	public Drinks(){
		super();
		
		setAlcoholPer(0);
		setCarbonate(false);
	}
	
	public Drinks(double price, boolean stock, double alcoholPer, boolean carbonate){
		super(price, stock);
		
		setAlcoholPer(alcoholPer);
		setCarbonate(carbonate);
	}
	
	public double getAlcoholPer() {
		return this.alcoholPer;
	}
	
	public void setAlcoholPer(double alcoholPer) {
		this.alcoholPer = alcoholPer;
	}
	
	public boolean isCarbonate() {
		return this.carbonate;
	}
	
	public void setCarbonate(boolean carbonate) {
		this.carbonate = carbonate;
	}

}
