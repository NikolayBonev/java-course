import java.util.Scanner;

public class NfactorialOverKfactorial {

	public static void main(String[] args) {
		Scanner inputs = new Scanner(System.in , "UTF-8");
		
		System.out.print("Insert N number: ");
		int numN=0;
		System.out.print("Insert K number: ");
		int numK=0;
		
		boolean insertNK=true, infinityStop=false;
		int numFactorial=1;
		
		for(;;){
			int i=0;
			while(insertNK){
				if(i==0){
//					System.out.print("Insert N number: ");
				}else if(i==1){
					System.out.print("Insert K number: ");
				}
				if(inputs.hasNextInt()){
					if(i==0){
						System.out.print("Insert N number: ");
						numN=inputs.nextInt();
						i++;
					}else if(i==1){
						numK=inputs.nextInt();
						i++;
					}
					
					if(numK>numN && i>1){
						System.out.println("N has to be greater than K!");
						i--;
					}else if(numK<=0){
						System.out.println("K has to be greather than zero less than N!");
						i++;
					}else if(numN==0){
						infinityStop=true;
						break;
					}else{
						insertNK=false;
						break;
					}
					
				}
			}
			
			for(numK++;numK<=numN && !(infinityStop==false);numK++){
				numFactorial*=numK;
				if(numK==numN){
					infinityStop=true;
				}
			}
			
			if(infinityStop==true){
				break;
			}
		}
		
		
		System.out.println("N!/K! = " + numFactorial);
		
		inputs.close();

	}

}
