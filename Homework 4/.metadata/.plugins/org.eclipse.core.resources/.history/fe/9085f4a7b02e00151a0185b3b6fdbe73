		for(int i=0, p=0;i<symArr.length;i++, p++){
			if(!(symArr[i].length==symArr.length)){
				System.out.println("The matrix is not symmetric!");
				break;
			}else{
				if(p==(symArr.length-1)){
					System.out.println("The matrix is symmetric!");
				}
			}
