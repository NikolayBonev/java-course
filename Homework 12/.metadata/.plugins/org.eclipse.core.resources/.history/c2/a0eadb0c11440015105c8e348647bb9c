public class InsertionSort {
	public void doInsertionSort(int[] inputArr){
        
        for (int i = 1; i < inputArr.length; i++) {
            for(int j = i ; j > 0 ; j--){
                if(inputArr[j] < inputArr[j-1]){
                    swap(inputArr, i, j);
                }
            }
        }
    }
	
	public void swap(int inputArr[], int i, int j){
    	
        int temp = inputArr[i];
        inputArr[i] = inputArr[j];
        inputArr[j] = temp; 
    }    
	
}
