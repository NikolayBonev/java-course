import java.util.Iterator;

public class Deque<E> implements Iterator<E> {
	private class Node<E> {
		public Node<E> left, right;
	    private final E item;

	    public Node(E item) {
	      if (item == null) { 
	    	  throw new NullPointerException();
	      }
	      
	      this.item = item;
	      this.right = null;
	    }
	    
	    public Node(E item, Node<E>) {
		      if (item == null) { 
		    	  throw new NullPointerException();
		      }
		      
		      this.item = item;
		      this.right = null;
		}

	    public void connectRight() {
	      this.right = nextElem;
	      nextElem.left = this;
	    }
	  }

	 
}
