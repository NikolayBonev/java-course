import java.util.Iterator;

public class Deq<E> implements Iterator<E> {
	private class Node<E> {
		public Node<E> left, right;
	    private final E item;

	    public Node(E item) {
	      if (item == null) { 
	    	  throw new NullPointerException();
	      }
	      
	      this.item = item;
	      this.right = null;
	    }
	    
	    public Node(E item, Node<E> prevElem) {
		      if (item == null) { 
		    	  throw new NullPointerException();
		      }
		      
		      this.item = item;
		      prevElem.right = this;
		      this.right = null;
		}
	}
	
	 private Node head;
	 private Node tail;
	 private int count;
	 
	 public Deq() {
		 this.head = null;
		 this.tail = null;
		 this.count = 0;
	 }
	 
	 public void add(Object item) {
		 if (head == null) {
			 head = new Node(item);
		     tail = head;
		 } else {
		     Node newNode = new Node(item, tail);
		     tail = newNode;
		    }
		 count++;
		}
}
