
public class Test {

	@SuppressWarnings("unchecked")
	public static void main(String[] args) {
		Tree<Integer> tree =
	            new Tree<Integer>(7,
	                    new Tree<Integer>(19,
	                          new Tree<Integer>(1),
	                          new Tree<Integer>(12),
	                          new Tree<Integer>(31)),
	                    new Tree<Integer>(21),
	                    new Tree<Integer>(14,
	                          new Tree<Integer>(23),
	                          new Tree<Integer>(6))
	    );

		int i = tree.searchElement(new Integer(7));
		
		System.out.println(i);
	}

}
